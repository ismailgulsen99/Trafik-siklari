void setup()
{
  pinMode(9, OUTPUT);
  pinMode(10, OUTPUT);
  pinMode(11, OUTPUT);
}

void loop()
{
  analogWrite(9, 255);
  delay(10000);
  analogWrite(10, 255);
  delay(10000);
  analogWrite(9, 0);
  delay(2000);
  analogWrite(10, 0);
  analogWrite(11, 255);
  delay(5000);
  analogWrite(11, 0);
}
